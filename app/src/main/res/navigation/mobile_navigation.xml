<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.myapplication0103.ui.reg.RegFragment"
        android:label="Вход"
        tools:layout="@layout/fragment_reg" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.myapplication0103.ui.acts.ActsFragment"
        android:label="Акты имплантации"
        tools:layout="@layout/fragment_acts" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.myapplication0103.ui.inventory.InventoryFragment"
        android:label="Инвентаризация"
        tools:layout="@layout/fragment_inventory" />

    <fragment
        android:id="@+id/navigation_help"
        android:name="com.example.myapplication0103.ui.help.HelpFragment"
        android:label="Помощь"
        tools:layout="@layout/fragment_help" />


</navigation>
